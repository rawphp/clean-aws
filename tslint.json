{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ]
  },
  "linterOptions": {
    "exclude": [
        "src/__tests__/fixtures/*"
    ]
  },
  "rulesDirectory": []
}
